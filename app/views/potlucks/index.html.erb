<div class="container">
  <div class="row">
    <div class="col-sm-12">
      <h1>Available Potlucks</h1>
      <ul class="potlucks">
        <% @potlucks.each do |potluck| %>
          <li><%= link_to potluck.start_time.strftime("%A %b %d, %Y | %r"),
            potluck_url(potluck) %></li>
        <% end %>
      </ul>
    </div>
  </div>
</div>

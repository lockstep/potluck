<div class="container">
  <div class="row">
    <div class="col-sm-12">
      <h1>Potluck</h1>
      <p>
        This potluck is scheduled to run from
        <%= @potluck.start_time.strftime("%A %b %d, %Y | %r") %>
        until
        <%= @potluck.end_time.strftime("%A %b %d, %Y | %r") %>.
      </p>
    </div>
  </div>
</div>
